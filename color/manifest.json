{
  "manifest_version":2,
  "name": "Color Elements",
  "description":"This extension gives each element unique colors",
  "version":"1.0",
  "browser_action":{
    "default_icon":"images/get_started16.png",
    "default_popup":"colors.html",
    "default_title":"My custom google page !"
  },
  "content_scripts": [
   {
     "matches": ["http://*/*","https://*/*"],
     "js": ["content.js"]
   }
 ]
}